Package: reptimod
Title: Reptile Integrated Distribution Modelling Tools
Version: 0.0.0.9000
Authors@R: c(
    person("Rebecca", "Turner", "bectur@ceh.ac.uk", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0001-5159-8266")), 
